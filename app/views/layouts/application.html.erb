<!DOCTYPE html>
<html>
  <head>
    <title>Gemgame</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => 'reload' %>
  </head>

  <body onload="time()">
    <div>
      <span id="timer" style="font-size:20px"></span>
    </div>
    <%= yield %>
  </body>
</html>
